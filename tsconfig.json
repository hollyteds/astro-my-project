{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@layouts/*": ["src/layouts/*"],
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@renderers/*": ["src/renderers/*"],
      "@images/*": ["src/images/*"],
      "@scss/*": ["src/scss/*"],
      "@js/*": ["src/js/*"]
    }
  }
}